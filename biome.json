{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true,
		"defaultBranch": "develop"
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": ["**/*.d.ts"]
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
            "correctness": {
                "noUnusedVariables": "error"
            }
		}
	}
}
